<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="DerpRaven.Maui.Views.PortfolioPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DerpRaven.Maui.Views"
    Title="">

    <TabBar >
        <Tab Title="Plushie Portfolio" Route="PlushiePortfolio" x:Name="PlushieTab" >
            <ShellContent ContentTemplate="{DataTemplate local:PlushiePortfolioPage}" />
        </Tab>
        <Tab Title="Art Portfolio" Route="ArtPortfolio" x:Name="ArtTab" >
            <ShellContent ContentTemplate="{DataTemplate local:ArtPortfolioPage}" />
        </Tab>
    </TabBar>
</Shell>
