<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DerpRaven.Maui.Views.CustomRequestPage"
             xmlns:local="clr-namespace:DerpRaven.Maui.Views"
             BackgroundColor="Grey">
    <TabBar>
        <Tab Title="Create">
            <ShellContent ContentTemplate="{DataTemplate local:CreateCustomRequestPage}" />
        </Tab>
        <Tab Title="View">
            <ShellContent ContentTemplate="{DataTemplate local:ViewCustomRequestsPage}" />
        </Tab>
    </TabBar>

</Shell>
